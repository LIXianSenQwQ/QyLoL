<Application x:Class="QyLoL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QyLoL"
             xmlns:pu="https://opensource.panuon.com/wpf-ui"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <pu:StyleDictionary Includes="All" />
                <ResourceDictionary>
                    <FontFamily x:Key="iconfont">/QyLoL;component/Resources/Fonts/#iconfont</FontFamily>
                </ResourceDictionary>
                <ResourceDictionary Source="/QyLoL;component/Style/ViewStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
